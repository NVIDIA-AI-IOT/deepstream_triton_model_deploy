name: "centerface"
platform: "onnxruntime_onnx"
max_batch_size: 0
input [
  {
    name: "input.1"
    data_type: TYPE_FP32
    format: FORMAT_NCHW
    dims: [  3, 480, 640]
    reshape { shape: [ 1, 3, 480, 640 ] }
  }
]

output [
  {
    name: "537"
    data_type: TYPE_FP32
    dims: [ 1, 1, 120, 160 ]
   # reshape { shape: [ 1, 1, 1, 1 ] }
    label_filename: "centerface_labels.txt"
  },
  {
    name: "538"
    data_type: TYPE_FP32
    dims: [ 1, 2, 120, 160]
    label_filename: "centerface_labels.txt"
  },

  {
    name: "539"
    data_type: TYPE_FP32
    dims: [1,  2, 120, 160]
    label_filename: "centerface_labels.txt"
  },
  {
    name: "540"
    data_type: TYPE_FP32
    dims: [1, 10 , 120, 160]
    label_filename: "centerface_labels.txt"
  }
]

instance_group {
  count: 1
  gpus: 0
  kind: KIND_GPU
}

# Enable TensorRT acceleration running in gpu instance. It might take several
# minutes during intialization to generate tensorrt online caches.

#optimization { execution_accelerators {
 # gpu_execution_accelerator : [ { name : "tensorrt" } ]
#		}}
